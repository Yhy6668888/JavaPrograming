public class TestCircleClassWithPrivateDataFields {
    public static void main(String[] args) {
        CircleWithPeivateDataFields circle1 = new CircleWithPeivateDataFields(5);
        System.out.println("Radius"+circle1.getRadius());
        System.out.println("Area"+circle1.getArea());
        CircleWithPeivateDataFields circle2 = new CircleWithPeivateDataFields(5);
        System.out.println("Radius"+circle2.getRadius());
        System.out.println("Area"+circle2.getArea());
        System.out.println("Number of objects created:"+CircleWithPeivateDataFields.getNumberOfObjects());
    }
}
