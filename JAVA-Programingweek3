public class day {
    public static void main(String[] args) {
        int currentDay = 2; // Tuesday is represented as 2 (assuming Sunday is 0, Monday is 1, and so on)
        int daysToAdd = 100;
        int resultDay = (currentDay + daysToAdd) % 7;
        switch (resultDay) {
            case 0:
                System.out.println("Sunday");
                break;
            case 1:
                System.out.println("Monday");
                break;
            case 2:
                System.out.println("Tuesday");
                break;
            case 3:
                System.out.println("Wednesday");
                break;
            case 4:
                System.out.println("Thursday");
                break;
            case 5:
                System.out.println("Friday");
                break;
            case 6:
                System.out.println("Saturday");
                break;
        }
    }
}




public class Test {
    public static void main(String[] args) {
        double i=50.0;
        double k=i+50.0;
        double j=k+1;


        System.out.println("j is "+j+" k is "+k);
    }
}


public class Test2 {
    public static void main(String[] args) {
        double radius=1.0;
        double area=Math.PI*radius*radius;
        System.out.println("The area of the circle is "+area+"for radious "+radius);

        double radius2=2.0;
        double area2=Math.PI*radius2*radius2;
        System.out.println("The area of the circle is "+area2+"for radious "+radius2);

    }
}


import java.util.Scanner;

public class Test3 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int k;
        k=sc.nextInt();
        int i=k+2;
        System.out.println(i);
    }
}


public class Test4 {
    public static void main(String[] args) {
        int i=2;
        int j=2;
        int k=2;
        i=j=k;
        System.out.println(i+" "+j+" "+k);
    }
}







